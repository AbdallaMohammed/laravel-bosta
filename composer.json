{
    "name": "abdallahmohammed/laravel-bosta",
    "description": "laravel-bosta",
    "keywords": [
        "almesery",
        "laravel-bosta"
    ],
    "homepage": "https://github.com/almesery/laravel-bosta",
    "license": "MIT",
    "authors": [
        {
            "name": "almesery",
            "email": "abdelrahman.pme18@suezuni.edu.eg",
            "role": "BackEnd Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.*",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/contracts": "^7.0|^8.0|^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.15"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.32",
        "vimeo/psalm": "^5"
    },
    "autoload": {
        "psr-4": {
            "Almesery\\Bosta\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Almesery\\Bosta\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Almesery\\Bosta\\BostaServiceProvider"
            ],
            "aliases": {
                "Bosta": "Almesery\\Bosta\\Facades\\BostaFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
