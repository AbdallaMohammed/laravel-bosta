{
    "name": "almesery/laravel-bosta",
    "description": "laravel-bosta",
    "keywords": [
        "almesery",
        "laravel-bosta"
    ],
    "homepage": "https://github.com/almesery/laravel-bosta",
    "license": "MIT",
    "authors": [
        {
            "name": "almesery",
            "email": "abdelrahman.pme18@suezuni.edu.eg",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.1",
        "illuminate/contracts": "^8.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Almesery\\Bosta\\": "src",
            "Almesery\\Bosta\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Almesery\\Bosta\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Almesery\\Bosta\\BostaServiceProvider"
            ],
            "aliases": {
                "Bosta": "Almesery\\Bosta\\BostaFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
