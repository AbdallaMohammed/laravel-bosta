{
    "name": "abdallahmohammed/laravel-bosta",
    "description": "laravel-bosta",
    "keywords": [
        "bosta",
        "shipping",
        "laravel-bosta"
    ],
    "homepage": "https://github.com/AbdallaMohammed/laravel-bosta",
    "license": "MIT",
    "authors": [
        {
            "name": "almesery",
            "email": "abdelrahman.pme18@suezuni.edu.eg",
            "role": "BackEnd Developer"
        },
        {
            "name": "AbdallahMohammed",
            "email": "abdallah.r660@gmail.com",
            "role": "Back-end Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/contracts": "^7.0|^8.0|^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.15"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.32",
        "vimeo/psalm": "^5"
    },
    "autoload": {
        "psr-4": {
            "LaravelBosta\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelBosta\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelBosta\\BostaServiceProvider"
            ],
            "aliases": {
                "Bosta": "LaravelBosta\\Facades\\BostaFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
